var datatable = $('#table').dataTable().api();

$.get('myUrl', function(newDataArray) {
    datatable.clear();
    datatable.rows.add(newDataArray);
    datatable.draw();
});


SELECT MENU_ID, MENU_NAME FROM SFC_MENU_MENUS ORDER BY MENU_ORDER
SELECT * FROM SFC_MENU_SECTIONS WHERE MENU_CAT_BASED = 9 ORDER BY MENU_ORDER
SELECT * FROM SFC_MENU_ITEMS WHERE MENU_BASED = 9 AND MENU_CAT = 5 ORDER BY MENU_ORDER

